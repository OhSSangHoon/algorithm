//Text의 모든 위치에서 시작되는 pattern을 검사
public class BFAlgorithm {
	
	public void search(String pat, String txt) {
		int M = pat.length(); //pattern 길이
		int N = txt.length(); //text 길이
		for(int i =0; i <= N; i++) {
			for(int j = 0; j < M; j++) {
				if(txt.charAt(i) != pat.charAt(i))
					break;
			if()
			}
		}
	}

