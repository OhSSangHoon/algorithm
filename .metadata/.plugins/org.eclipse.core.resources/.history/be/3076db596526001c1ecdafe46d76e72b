package concert;
import java.util.Scanner;


public class concert {

	public static void main(String[] args) {
		concert sr = new concert(); //concert 객체 생성
		Scanner sc = new Scanner(System.in);
		int select;
		
		while(true) {
			System.out.print("예약<1>, 조회<2>, 취소<3>, 끝내기<4> >>");
			
			select = sc.nextInt();
		}
	}

}
